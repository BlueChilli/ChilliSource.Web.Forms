{
  "name": "chillisource.web.forms",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.ts",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "dist": "webpack --config webpack.build.config.js -p"
  },
  "author": "BlueChilli",
  "license": "ISC",
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-object-assign",
      "transform-react-display-name"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.s?css$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transform": {
      "^.+\\.tsx?$": "typescript-babel-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
